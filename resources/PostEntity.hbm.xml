<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Career.PostEntity" table="post" schema="development">
        <id name="idPost">
            <column name="id_post" sql-type="int(11)"/>
        </id>
        <property name="namePost">
            <column name="name_post" sql-type="varchar(145)" length="145"/>
        </property>
    </class>
</hibernate-mapping>