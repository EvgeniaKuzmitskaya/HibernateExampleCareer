<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Career.FormEntity" table="form" schema="development">
        <id name="idForm">
            <column name="id_form" sql-type="int(11)"/>
        </id>
        <property name="nameForm">
            <column name="name_form" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="surnameForm">
            <column name="surname_form" sql-type="varchar(75)" length="75"/>
        </property>
        <property name="pfntronicName">
            <column name="pfntronic_name" sql-type="varchar(85)" length="85"/>
        </property>
    </class>
</hibernate-mapping>