<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Career.WorkEntity" table="work" schema="development">
        <id name="idWork">
            <column name="id_work" sql-type="int(11)"/>
        </id>
        <property name="recruitWork">
            <column name="recruit_work" sql-type="date"/>
        </property>
        <property name="sackWork">
            <column name="sack_work" sql-type="date"/>
        </property>
    </class>
</hibernate-mapping>